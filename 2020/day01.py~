#!/usr/bin/python3

import sys

with open(sys.argv[0]) as in:
    data = in.readlines()

for i in range(len(data)):
    for element in data[i+1:]:
        if(data[i] + element == 2020):
            print "i=%d data[i]=%d element=%d i+element=%d i*element=%d".format(i, data[i], element, i+element, i*element)
